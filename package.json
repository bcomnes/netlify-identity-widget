{
  "name": "netlify-identity-widget",
  "version": "1.4.4",
  "releaseVersion": "v1",
  "description": "Netlify Identity widget for easy integration",
  "scripts": {
    "start": "run-s watch",
    "prepare": "run-s build",
    "build": "run-s module widget",
    "widget": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
    "module": "run-s module:*",
    "module:clean": "rimraf dist && mkdirp dist",
    "module:css": "postcss css/modal.css -o src/bundle.css --config postcss.config.js",
    "module:js": "babel src --out-dir dist --source-maps --copy-files",
    "watch": "run-p watch:*",
    "watch:css": "npm run module:css -- --watch",
    "watch:js": "npm run module:js -- --watch --skip-initial-build",
    "watch:widget": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config webpack.config.babel.js",
    "clean": "rimraf dist && mkdirp dist",
    "prebuild": "rimraf build && mkdirp build",
    "test": "eslint src",
    "version": "node ./script/release.js",
    "format": "prettier --write 'src/**/*.js'",
    "format-preview": "prettier --list-different 'src/**/*.js'",
    "preview-site": "serve build -s -c 1"
  },
  "main": "./dist/index.js",
  "keywords": [
    "netlify",
    "identity",
    "authentication"
  ],
  "license": "MIT",
  "author": "Matt Biilmann <matt@netlify.com>",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-static-fs": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-postcss": "^0.3.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.11.6",
    "caniuse-lite": "^1.0.30000787",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.1",
    "postcss": "^6.0.16",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cli": "^4.1.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.3",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.0",
    "prettier": "^1.6.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "serve": "^6.0.0",
    "source-map-loader": "^0.2.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.4.4",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "gotrue-js": "^0.9.15",
    "insert-css": "^2.0.0",
    "mobx": "^3.2.2",
    "mobx-preact": "^1.1.0",
    "preact": "^8.1.0"
  }
}
